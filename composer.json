{
    "name": "madewithlove/definitions",
    "description": "Definitions for common packages for definition-interop compatible projects",
    "license": "MIT",
    "authors": [
        {
            "name": "madewithlove",
            "email": "heroes@madewithlove.be",
            "role": "Organization"
        },
        {
            "name": "Maxime Fabre",
            "email": "maxime@madewithlove.be",
            "role": "Developper"
        }
    ],
    "autoload": {
        "psr-4": {
            "Madewithlove\\Definitions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Madewithlove\\Definitions\\": "tests"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "league/container": "^2.0",
        "mnapoli/assembly": "dev-master"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "2.0.x@dev",
        "illuminate/cache": "^5.1",
        "illuminate/database": "^5.1",
        "illuminate/filesystem": "^5.1",
        "league/flysystem": "^1.0",
        "league/route": "2.0.x@dev",
        "league/tactician": "^0.6.1",
        "maximebf/debugbar": "^1.10",
        "mockery/mockery": "^0.9.4",
        "monolog/monolog": "^1.17",
        "phpunit/phpunit": "^5.0.9",
        "relay/relay": "^1.0",
        "symfony/console": "^2.8",
        "twig/twig": "^1.23",
        "zendframework/zend-diactoros": "^1.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
