{
    "name": "madewithlove/service-providers",
    "description": "Service providers for common packages for interop service-provider compatible projects",
    "license": "MIT",
    "authors": [
        {
            "name": "madewithlove",
            "email": "heroes@madewithlove.be",
            "role": "Organization"
        },
        {
            "name": "Maxime Fabre",
            "email": "maxime@madewithlove.be",
            "role": "Developper"
        }
    ],
    "autoload": {
        "psr-4": {
            "Madewithlove\\ServiceProviders\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Madewithlove\\ServiceProviders\\": "tests"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "container-interop/service-provider": "dev-master",
        "league/container": "^2.4"
    },
    "require-dev": {
        "illuminate/cache": "^5.1",
        "illuminate/database": "^5.1",
        "illuminate/filesystem": "^5.1",
        "league/event": "^2.1",
        "league/factory-muffin": "^2.1",
        "league/flysystem": "^1.0",
        "league/route": "2.0.x@dev",
        "league/tactician": "^0.6.1",
        "madewithlove/php-cs-fixer-config": "^1.2",
        "maximebf/debugbar": "^1.10",
        "mockery/mockery": "^0.9.4",
        "monolog/monolog": "^1.17",
        "phpunit/phpunit": "^5.0.9",
        "relay/relay": "^1.0",
        "symfony/console": "^2.8",
        "twig/twig": "^1.23",
        "zendframework/zend-diactoros": "^1.2"
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
